  "meta": {
    "name": "Anthropic Warm Humanist",
    "version": "1.0.0",
    "description": "A design system inspired by Anthropic's illustration language and Geist's brand execution. Combines warm earth-tone palettes with organic-modernist composition, paper-like grain textures, and editorial typography to create a UI that feels handcrafted, intelligent, and human-centered. Designed for fintech and data-rich applications that need to feel trustworthy without feeling sterile.",
    "mood_keywords": [
      "warm-intellectual",
      "handcrafted-precision",
      "grounded-optimism",
      "human-centered-tech",
      "quiet-confidence",
      "considered-care"
    ],
    "style_references": [
      "organic modernism",
      "warm brutalism",
      "neo-editorial",
      "humanist constructivism",
      "risograph print aesthetic"
    ],
    "color_extraction_method": "inferred from artwork tile analysis + Geist case study palette references"
  },

