    "md": "0 2px 8px rgba(45, 40, 36, 0.06), 0 1px 2px rgba(45, 40, 36, 0.04)",
    "lg": "0 4px 16px rgba(45, 40, 36, 0.08), 0 2px 4px rgba(45, 40, 36, 0.04)",
    "xl": "0 8px 32px rgba(45, 40, 36, 0.1), 0 4px 8px rgba(45, 40, 36, 0.04)",
    "focus": "0 0 0 3px rgba(196, 123, 90, 0.2)",
    "inner": "inset 0 1px 2px rgba(45, 40, 36, 0.06)",
    "note": "All shadows use warm brown base (45, 40, 36) rather than pure black for cohesion with the earth-tone palette"
  },

  "motion": {
    "durations": {
      "instant": "100ms",
      "fast": "150ms",
      "normal": "250ms",
      "slow": "400ms",
      "slower": "600ms"
    },
    "easing": {
      "default": "cubic-bezier(0.25, 0.1, 0.25, 1.0)",
      "ease_out": "cubic-bezier(0.16, 1, 0.3, 1)",
      "ease_in": "cubic-bezier(0.55, 0, 1, 0.45)",
      "ease_in_out": "cubic-bezier(0.65, 0, 0.35, 1)",
      "gentle": "cubic-bezier(0.4, 0, 0.2, 1)"
    },
    "component_guidelines": {
      "hover": {
        "duration": "fast",
        "easing": "default",
        "properties": "background-color, border-color, box-shadow, transform",
        "notes": "Subtle darken (8%) + optional scale(1.01) for cards. No color flashing."
      },
      "focus": {
        "duration": "instant",
        "easing": "default",
        "properties": "box-shadow, outline",
        "notes": "Terracotta focus ring appears immediately. No transition delay."
      },
      "modal_enter": {
        "duration": "normal",
        "easing": "ease_out",
        "properties": "opacity, transform",
        "notes": "Fade in from 0 + scale from 0.97 to 1.0. Backdrop fades in at 'slow' duration."
      },
      "modal_exit": {
        "duration": "fast",
        "easing": "ease_in",
        "properties": "opacity, transform",
        "notes": "Fade out + scale to 0.97. Exits should always be faster than enters."
      },
      "page_transition": {
        "duration": "slow",
        "easing": "ease_out",
        "properties": "opacity, transform",
        "notes": "Content fades in with 12px upward drift. Stagger children by 50ms."
      },
      "accordion": {
        "duration": "normal",
        "easing": "ease_out",
        "properties": "height, opacity",
        "notes": "Content height animates with opacity. Chevron rotates 180deg."
      },
      "tab_indicator": {
        "duration": "normal",
        "easing": "ease_out",
        "properties": "left, width",
        "notes": "Underline slides to new tab position. Width morphs if tabs differ."
      }
    },
    "reduced_motion": {
      "behavior": "Replace all transforms and sliding with instant opacity changes. Disable grain animation. Honor prefers-reduced-motion media query."
    }
  },

  "textures": {
    "grain": {
      "description": "SVG noise filter applied as a pseudo-element overlay on backgrounds. Creates the risograph/paper-like tactile quality central to the Anthropic illustration style.",
      "opacity_light": "0.035",
      "opacity_dark": "0.04",
      "css_filter": "url(#grain-filter)",
      "svg_filter_def": "<filter id='grain-filter'><feTurbulence type='fractalNoise' baseFrequency='0.65' numOctaves='3' stitchTiles='stitch'/><feColorMatrix type='saturate' values='0'/></filter>",
      "implementation_note": "Apply via ::after pseudo-element with pointer-events: none, full absolute positioning, mix-blend-mode: multiply (light) or soft-light (dark)."
    }
  },

